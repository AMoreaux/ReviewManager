{% extends 'EmiageReviewManagerBundle::layout.html.twig' %}

{% block content -%}
    <h1>Examen edit</h1>
    <div class="input-group-addon">
        <div class="container">
            <div class="form-group ">
                {{ form_start(edit_form) }}
                {{ form_row(edit_form.realizedOn, { 'attr': { 'class': 'form-control', 'name': 'date'}}) }}
                {{ form_row(edit_form.module, { 'attr': { 'class': 'form-control' }}) }}
                {{ form_row(edit_form.reviewCenter, { 'attr': { 'class': 'form-control' }}) }}
                {{ form_row(edit_form.submit, { 'attr': { 'class': 'btn btn-success' }}) }}
                {{ form_end(edit_form) }}
            </div>
        </div>
    </div>
{% endblock %}
