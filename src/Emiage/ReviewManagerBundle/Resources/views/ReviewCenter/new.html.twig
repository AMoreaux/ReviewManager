{% extends 'EmiageReviewManagerBundle::layout.html.twig' %}

{% block content -%}
    <h1>ReviewCenter creation</h1>
    <tr>
        <form method="post" {{ form_enctype(form) }} class="input-group input-group-lg">
            <span class="input-group-addon">
               <td> {{ form_label(form.name, ' name') }}</td>
               <td> {{ form_widget(form.name) }}</td>
               <td> {{form_rest(form)}}</td>
            </span>
        </form>
    </tr>
    <ul class="record_actions">
        <li>
            <a href="{{ path('reviewcenter') }}" class="btn btn-default">
                Back to the list
            </a>
        </li>
    </ul>
{% endblock %}
